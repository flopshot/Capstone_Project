<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/match_item"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/clubName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
        tools:text="Angel Park"/>

    <TextView
        android:id="@+id/courseName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/clubName"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="Cloud 9"/>

    <TextView
        android:id="@+id/roundDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/courseName"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="04/01/17"/>

    <TextView
        android:id="@+id/p1One"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:layout_below="@id/roundDate"
        android:layout_alignParentStart="true"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
        android:text="@string/p1Symbol"/>

    <TextView
        android:id="@+id/p1First"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/roundDate"
        android:layout_toEndOf="@id/p1One"
        android:layout_alignBaseline="@id/p1One"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="Matt"/>

    <TextView
        android:id="@+id/p3Three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="8dp"
        android:layout_below="@id/p1One"
        android:layout_alignParentStart="true"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
        android:text="@string/p3Symbol"/>

    <TextView
        android:id="@+id/p3First"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/roundDate"
        android:layout_toEndOf="@id/p3Three"
        android:layout_alignBaseline="@id/p3Three"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="Matt"/>

    <TextView
        android:id="@+id/p2Two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="8dp"
        android:layout_below="@id/roundDate"
        android:layout_alignParentEnd="true"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
        android:text="@string/p2Symbol"/>

    <TextView
        android:id="@+id/p2First"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/roundDate"
        android:layout_toStartOf="@id/p2Two"
        android:layout_alignBaseline="@id/p2Two"
        android:gravity="center|end"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="Matt"/>

    <TextView
        android:id="@+id/p4Four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="8dp"
        android:layout_below="@id/p2Two"
        android:layout_alignParentEnd="true"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
        android:text="@string/p4Symbol"/>

    <TextView
        android:id="@+id/p4First"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center|end"
        android:layout_below="@id/p2First"
        android:layout_toStartOf="@id/p4Four"
        android:layout_alignBaseline="@id/p4Four"
        android:ems="8"
        android:lines="1"
        android:ellipsize="end"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Subhead"
        tools:text="Matt"/>

</RelativeLayout>